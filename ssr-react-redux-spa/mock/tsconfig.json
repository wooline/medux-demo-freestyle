{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "lib": ["es2018"],
    "types": ["node"],
    "allowJs": true,
    "checkJs": false,
    "resolveJsonModule": true,
    "strict": true,
    "importHelpers": true,
    "declaration": false,
    "alwaysStrict": true,
    "noImplicitAny": false,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "rootDir": "./",
    "outDir": "../dist/mock",
    "paths": {
      "~/*": ["../src/*"]
    }
  },
  "include": ["./"],
  "exclude": []
}


