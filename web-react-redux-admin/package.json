{
  "name": "ssr-react-helloworld",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "checkType": "tsc",
    "eslint": "eslint --cache .",
    "eslintSrc": "eslint --cache ./src",
    "stylelint": "stylelint \"./src/**/*.less\"",
    "checkCode": "yarn checkType & yarn eslint & yarn eslintSrc",
    "build": "yarn checkCode && cross-env NODE_ENV=production PROJ_ENV=common node build/build.js",
    "start": "yarn checkCode && cross-env NODE_ENV=development PROJ_ENV=common node build/start.js",
    "debug": "cross-env NODE_ENV=development PROJ_ENV=common DEBUG=true node build/start.js",
    "runDist": "node ./dist/common/start.js",
    "debug2": "ts-node --inspect=19248 --debug-brk typescript-json-schema-cli.ts",
    "patchActions": "medux-patch-actions patch"
  },
  "private": "true",
  "workspaces": [
    "public"
  ],
  "browserslist": [
    "ie >= 11"
  ],
  "author": "",
  "license": "ISC",
  "resolutions": {
    "@types/react": "~16.9.23",
    "@types/react-dom": "~16.9.5"
  },
  "dependencies": {
    "@medux/react-web-router": "2.0.0-beta.3.2",
    "axios": "~0.21.0",
    "react": "~17.0.1",
    "react-dom": "~17.0.1",
    "react-redux": "~7.2.2"
  },
  "devDependencies": {
    "@types/jest": "~26.0.3",
    "@types/react": "~16.9.23",
    "@types/react-dom": "~16.9.5",
    "@types/react-redux":"~7.1.14",
    "@medux/dev-pkg": "~2.0.6",
    "@medux/dev-webpack": "~1.0.10",
    "husky": "~4.3.0",
    "jest": "~26.6.3",
    "less": "~3.12.2",
    "less-loader": "~7.1.0",
    "lint-staged": "~10.5.1",
    "typescript": "~4.0.5"
  }
}
